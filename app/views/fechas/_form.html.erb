<%= form_for(@fecha) do |f| %>
  <% if @fecha.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@fecha.errors.count, "error") %> prohibited this fecha from being saved:</h2>

      <ul>
      <% @fecha.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :Fecha %><br>
    <%= f.date_select :fecha %>
  </div>
  <label>Empleados</label>
  <table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rut</th>
      <th>Email</th>
      <th>Sueldo</th>
      <th>Fecha ingreso</th>
    </tr>
  </thead>

  <tbody>
    <% empleados.each do |employee| %>
      <tr>
        <td><%= check_box_tag "employees[]", matter.id %><%= employee.nombre %></td>
        <td><%= employee.rut %></td>
        <td><%= employee.email %></td>
        <td><%= employee.sueldo %></td>
        <td><%= employee.fecha_ingreso %></td>td>
      </tr>
    <% end %>
  </tbody>
</table>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
