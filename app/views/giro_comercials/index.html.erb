<p id="notice"><%= notice %></p>

<h1>Listando Giros Comerciales</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @giro_comercials.each do |giro_comercial| %>
      <tr>
        <td><%= giro_comercial.nombre %></td>
        <td><%= link_to 'Mostrar', giro_comercial %></td>
        <td><%= link_to 'Editar', edit_giro_comercial_path(giro_comercial) %></td>
        <td><%= link_to 'Destruir', giro_comercial, method: :delete, data: { confirm: 'EstÃ¡ seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Giro comercial', new_giro_comercial_path, class: "btn btn-primary margenes" %>
